                printf("6. Voltar ao menu principal  \n");
                scanf("%d",&men2);
                                    if(men2==1){
                                        printf("Digite os minutos:\n");
                                        scanf("%f",&minuto[1][0]);
                                        printf("Digite os Segundos:\n");
